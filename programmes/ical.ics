---
layout: ical
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:{{ site.baseurl }}
CALSCALE:GREGORIAN
NAME:{{ site.name }}
X-WR-CALNAME:{{ site.name }}
REFRESH-INTERVAL;VALUE=DURATION:PT1H
X-PUBLISHED-TTL:PT1H
METHOD:PUBLISH
BEGIN:VTIMEZONE
TZID:{{ site.timezone }}
TZURL:http://tzurl.org/zoneinfo-outlook/{{ site.timezone }}
X-LIC-LOCATION:{{ site.timezone }}
BEGIN:STANDARD
TZOFFSETFROM:+0800
TZOFFSETTO:+0800
TZNAME:{{ site.time | date: "%Z" }}
DTSTART:19700101T000000
END:STANDARD
END:VTIMEZONE
BEGIN:VEVENT
DTSTAMP:{{ site.time | date: "%Y%m%dT%H%M%SZ" }}
UID:{{ site.cfrc_start | date: "%Y%m%dT%H%M%SZ" }}-{{ site.cfrc_end | date: "%Y%m%dT%H%M%SZ" }}
DTSTART;VALUE=DATE:{{ site.cfrc_start | date: "%Y%m%d" }}
DTEND;VALUE=DATE:{{ site.cfrc_end | date: "%Y%m%d" }}
SUMMARY:{{ site.name }}
URL:{{ site.baseurl }}
DESCRIPTION:{{ site.name }}: {{ site.baseurl }}
LOCATION:{{ site.cfrc_place }}
END:VEVENT
{% assign entries=site.pages | where:"layout", "event" %}{% for item in entries %}{% if item.lang == "en" %}BEGIN:VEVENT
DTSTAMP:{{ item.start | date: "%Y%m%dT%H%M%SZ" }}
UID:{{ item.start | date: "%Y%m%dT%H%M%SZ" }}-{{ item.lang }}-{{ item.ref }}
DTSTART;TZID=Asia/Shanghai:{{ item.start | date: "%Y%m%dT%H%M%S" }}
DTEND;TZID=Asia/Shanghai:{{ item.end | date: "%Y%m%dT%H%M%S" }}
SUMMARY:{{ item.title }}
DESCRIPTION:See {{ site.baseurl }}{{ item.url }}
URL:{{ site.baseurl }}{{ item.url }}
LOCATION:{{ item.place }}
END:VEVENT
{% endif %}{% endfor %}END:VCALENDAR
