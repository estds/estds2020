---
search_exclusion: yes
---
[{% assign allevents=site.pages | where: "layout","events" %}{% assign entries=allevents | where: "lang","en" %}{% for item in entries %}{% if item.lang == "en" %}{"title": "{{ item.title }}","start": "{{ item.start | date: "%Y-%m-%dT%H:%M:%S" }}","end": "{{ item.end | date: "%Y-%m-%dT%H:%M:%S" }}","url":"{{ site.baseurl }}{{ item.url }}"}{% unless forloop.last %},{% endunless %}{% endfor %}]
